{
	"name": "taro-h5-rn-weapp",
	"author": "MiniOcean404",
	"version": "1.0.0",
	"private": true,
	"description": "Taro-React模板",
	"templateInfo": {
		"name": "default",
		"typescript": true,
		"css": "sass"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"scripts": {
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn-android -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:jd": "npm run build:jd -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn-android": "taro build --type rn --platform android",
		"build:rn-ios": "taro build --type rn --platform ios",
		"build:qq": "taro build --type qq",
		"build:jd": "taro build --type jd",
		"build:quickapp": "taro build --type quickapp"
	},
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@tarojs/components": "3.5.12",
		"@tarojs/plugin-framework-react": "3.5.12",
		"@tarojs/react": "3.5.12",
		"@tarojs/rn-runner": "3.5.12",
		"@tarojs/runtime": "3.5.12",
		"@tarojs/taro": "3.5.12",
		"@tarojs/taro-rn": "3.5.12",
		"react": "^17.0.2",
		"react-dom": "^17.0.0",
		"react-native": "^0.66.0",
		"three": "^0.138.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/mini-runner": "3.5.12",
		"@tarojs/webpack-runner": "3.5.12",
		"@types/react": "^17.0.2",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"babel-preset-taro": "3.5.12",
		"eslint": "^6.8.0",
		"eslint-config-taro": "3.5.12",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"prettier": "^2.5.1",
		"stylelint": "9.3.0",
		"taro-ui": "^3.1.0-beta.2",
		"typescript": "^4.1.0"
	}
}
